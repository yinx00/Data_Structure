#include<iostream>
using namespace std;
int main()
{
	int a[100];
	int n,i=0;
	while (cin >> n)
	{
		a[i] = n;
		++i;
	}
	for (int j = 1; j != i; ++j)
	{
		int tem;
		for (int k = j; k != 0; --k)
		{
			if (a[k] > a[k - 1])
			{
				tem = a[k];
				a[k] = a[k - 1];
				a[k - 1] = tem;
			}
		}
	}
	for (int k = 0; k < i; k++)
	{
		cout << a[k] << " ";
	}
	cout << endl;
	return 0;
}
